{
  "house_automation":
  {
    "value": false, //mettre à true quand software, rooms et sensors sont à true
    "software":
    {
      "value": false, //mettre à true quand au moins un management est true
      "management_heating":
      {
        "value": false, //mettre à true quand présent
        "constraint": ["temperature"] //vérifier la constrainte, si pas présent envoyer une erreur. Du coup il faut d'abord parcourir les sensors
      },
      "management_air_conditioning":
      {
        "value": false,
        "constraint": ["temperature"]
      },
      "management_light":
      {
        "value": false,
        "constraint": ["light"]
      },
      "management_humidity":
      {
        "value": false,
        "constraint": ["humidity"]
      },
      "management_windows":
      {
        "value": false,
        "constraint": ["humidity", "pollution", "sonore", "temperature", "wind"]
      },
      "management_hifi":
      {
        "value": false,
        "constraint": ["movement", "camera"]
      },
      "management_tv":
      {
        "value": false,
        "constraint": ["movement", "camera"]
      },
      "management_ventilation":
      {
        "value": false,
        "constraint": ["pollution", "humidity"]
      },
      "management_shutter_window":
      {
        "value": false,
        "constraint": ["management_windows"]
      }
    },
    "sensors":
    {
      "value": false, //mettre à true quand au moins un sensor est à true
      "humidity": false, //mettre à true quand présent
      "light": false,
      "temperature": false,
      "pollution": false,
      "wind": false,
      "sonore": false,
      "movement": false,
      "camera": false
    },
    "rooms":
    {
      "value": false, //mettre à true quand toutes les rooms mandatory sont true
      "garden":
      {
        "mandatory": false, //ne pas modifier les mandatory
        "value": false //mettre à true quand présent
      },
      "kitchen":
      {
        "mandatory": true,
        "value": false
      },
      "living_room":
      {
        "mandatory": true,
        "value": false
      },
      "bedroom":
      {
        "mandatory": true,
        "value": false
      },
      "bathroom":
      {
        "mandatory": true,
        "value": false
      },
      "laundry_room":
      {
        "mandatory": false,
        "value": true
      },
      "entry":
      {
        "mandatory": false,
        "value": false
      },
      "dinning_room":
      {
        "mandatory": false,
        "value": false
      },
      "game_room":
      {
        "mandatory": false,
        "value": false
      }
    }
  }
}